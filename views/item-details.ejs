<!--include header-->
<%- include ('include/_header')%> 
<!--/include header-->


<main id="site-main">
    <div class="container">
        <%- include ('include/_back')%>
        <br>
        <table class="table table-hover">
          <thead class="table-primary">
              <tr>
                  <th>Title</th>
                  <th>Description</th>
                  <th>Price</th>
              </tr>
          </thead>
          <tbody>
          <tr>
              <td>
                  <%= item.title%>
              </td>
              <td style="word-break:break-all;">
                <%= item.description %>
              </td>

              <td>
                  <%= item.price %>
              </td>
          </tr>
        </tbody>
        </table>

</main>

<!--include footer-->
<%- include ('include/_footer')%>
<!--/include foter-->
